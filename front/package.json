{
  "name": "front",
  "version": "1.0.0",
  "licence": "PROPRIETARY",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --hmr",
    "devssl": "ng serve --host 0.0.0.0 --hmr --ssl",
    "start": "ng s --proxy-config proxy.conf.json",
    "buildold": "npm run ng-high-memory build",
    "build": "ng build --configuration production",
    "esbuild.old": "esbuild ./src/main.ts --bundle --minify --sourcemap --outfile=./dist/run.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "hmr": "ng s --proxy-config proxy.conf.json --hmr",
    "analyze": "npm run ng-high-memory build -- --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "prepare": "cd .. && husky front/.husky",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.1",
    "@angular/cdk": "21.1.1",
    "@angular/common": "^21.1.1",
    "@angular/compiler": "^21.1.1",
    "@angular/core": "^21.1.1",
    "@angular/forms": "^21.1.1",
    "@angular/platform-browser": "^21.1.1",
    "@angular/platform-browser-dynamic": "^21.1.1",
    "@angular/pwa": "^21.1.1",
    "@angular/router": "^21.1.1",
    "@angular/service-worker": "^21.1.1",
    "@ant-design/icons-angular": "^21.0.0",
    "@delon/abc": "^21.0.4",
    "@delon/acl": "^21.0.4",
    "@delon/auth": "^21.0.4",
    "@delon/cache": "^21.0.4",
    "@delon/chart": "^21.0.4",
    "@delon/form": "^21.0.4",
    "@delon/mock": "^21.0.4",
    "@delon/theme": "^21.0.4",
    "@delon/util": "^21.0.4",
    "@geoapify/geocoder-autocomplete": "^2.1.0",
    "@ngx-translate/core": "^16.0.4",
    "@slickgrid-universal/common": "^9.11.0",
    "@slickgrid-universal/event-pub-sub": "^9.11.0",
    "@slickgrid-universal/excel-export": "^9.11.0",
    "@slickgrid-universal/row-detail-view-plugin": "^9.11.0",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.2",
    "@zxing/ngx-scanner": "^21.0.0",
    "ajv-formats": "^3.0.1",
    "angular-slickgrid": "^9.11.0",
    "file-saver": "^2.0.5",
    "ng-zorro-antd": "^21.0.2",
    "ngx-extended-pdf-viewer": "^25.6.4",
    "ngx-image-cropper": "^6.3.4",
    "ngx-mask": "^20.0.3",
    "ngx-tinymce": "^18.0.0",
    "pdf-lib": "^1.17.1",
    "qrious": "^4.0.2",
    "rxjs": "~7.8.1",
    "screenfull": "^6.0.2",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.1",
    "@angular/cli": "^21.1.1",
    "@angular/compiler-cli": "^21.1.1",
    "@angular/language-service": "^21.1.1",
    "@delon/testing": "^18.1.0",
    "@ng-util/monaco-editor": "^18.0.0",
    "@types/dompurify": "^3.0.5",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~4.3.6",
    "@types/jasminewd2": "~2.0.13",
    "@types/sortablejs": "^1.15.8",
    "angular-eslint": "21.1.0",
    "eslint": "^9.39.1",
    "husky": "^9.1.5",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.3.0",
    "ng-alain": "^19.0.1",
    "ng-alain-plugin-theme": "^18.0.0",
    "node-fetch": "^2.7.0",
    "prettier": "^3.3.3",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-order": "^6.0.4",
    "ts-node": "~10.9.2",
    "tslib": "^2.6.3",
    "typescript": "5.9.3",
    "typescript-eslint": "8.47.0"
  },
  "lint-staged": {
    "(src)/**/*.{html,ts}": [
      "eslint --fix"
    ],
    "(src)/**/*.less": [
      "stylelint --syntax less --fix"
    ]
  }
}
